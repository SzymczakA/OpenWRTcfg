
config wifi-device 'radio0'
	option type 'mac80211'
	option path '10180000.wmac'
	option country 'CN'
	option txpower '20'
	option hwmode '11ng'
	option htmode 'HT40+'
	option noscan '1'
	option channel '6'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option key 'goodlife'
	option encryption 'psk-mixed'
	option ifname 'wlan0'
	option ssid 'GL-MT300N-175'
	option wds '1'
	option macaddr 'e4:95:6e:42:d1:77'

config wifi-device 'radio1'
	option type 'mac80211'
	option path '101c0000.ehci/usb1/1-1/1-1:1.0'
	option htmode 'HT20'
	option hwmode '11g'
	option txpower '20'
	option channel '1'
	option country 'PL'

config wifi-iface 'sta'
	option mode 'sta'
	option network 'wwan'
	option bssid 'A6:EC:80:4C:71:BE'
	option ifname 'wlan-sta'
	option encryption 'psk2'
	option wds '0'
	option ssid 'Galaxy S6'
	option device 'radio1'
	option key 'nomore70'
	option disabled '0'

